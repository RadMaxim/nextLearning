generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(ulid())
  email    String @unique
  password String

  created DateTime @default(now()) @map("created")

  @@map("user")
}
